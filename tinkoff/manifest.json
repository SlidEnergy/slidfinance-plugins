{
  "manifest_version": 2,
  "name": "Myfinance.TinkoffBank",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*.tinkoff.ru/*",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Тинькофф банк\n\nЭкспорт баланса и транзакций в систему myfinance."
  }
}