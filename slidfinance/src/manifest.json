{
  "name": "SlidFinance",
  "version": "1.0",
  "description": "Экспорт баланса и транзакций в систему SlidFinance",
  "manifest_version": 2,
  "permissions": [
    "https://myfinance-frontend.herokuapp.com/",
    "activeTab",
    "webNavigation",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "runtime.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "assets/angular.png",
    "default_title": "Экспорт баланса и транзакций в систему SlidFinance."
  }
}
