{
  "manifest_version": 2,
  "name": "Myfinance.Qiwi",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*.qiwi.com/*",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Qiwi\n\nЭкспорт баланса и транзакций в систему slidfinance."
  }
}
